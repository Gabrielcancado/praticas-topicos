#include <iostream>

using namespace std;

int main()
{
    float x;
    float y;
    cout << "insira uma coordenada para o eixo x" << endl;
    cin >> x;
    cout << "insira uma coordenada para o eixo y" << endl;
    cin >> y;
    if(x<0 || x>=1 || y<0 || y>1 || (x*x) + (y*y)>=1){
        cout << "o ponto inserido nao pertence a figura H" << endl;

        return 0;
    } else {
        cout << "o ponto pertence a figura H" << endl;
    }

    return 0;
}
