#include <iostream>

using namespace std;

int main()
{
    int a, b, n;
    float h, soma=0;
    cout << "Insira onde o intervalo que deseja a area inicia" << endl;
    cin >> a;
    cout << "Insira onde o intervalo que deseja a area se encerra" << endl;
    cin >> b;
    cout << "Insira um valor para n" << endl;
    cin >> n;
    h = (b - a) / n;
    cout << h;
    return 0;
}
